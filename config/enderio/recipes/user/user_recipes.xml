    <recipe name="Tweak: Stick from Wood" disabled="true" required="false">
    <crafting>
      <grid size="1x2">
        <item name="logWood"/>
        <item name="logWood"/>
      </grid>
      <output name="minecraft:stick, stickWood" amount="16" />
    </crafting>
  </recipe>

